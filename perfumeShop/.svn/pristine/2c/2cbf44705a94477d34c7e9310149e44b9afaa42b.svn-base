function tagCreate() {
	document.f.action = "tag_write_action";
	document.f.method='POST';
	document.f.submit();
}
function tagCreateform() {
	document.f.action = "tag_write_form";
	document.f.method='POST';
	document.f.submit();
}
function tagRemove() {
	if (window.confirm("정말 삭제하시겠습니까?")) {
		document.f.action = 'tag_delete_action';
		document.f.method = 'POST';
		document.f.submit();
	}
}
function tagModifyAction()  {
	document.f.action = 'tag_update_action';
	document.f.method = 'POST';
	document.f.submit();
}
function tagList() {
	document.f.action = "tag_list";
	document.f.method='POST';
	document.f.submit();
}
